<% if user_signed_in?%>
  <% if !(@service.creator_id==current_user.id) %>
          <% if @follower.size == 0%>
                  <%= link_to 'Follow', follow_path(@service) %>
          <%else%>
                  <%= link_to 'Unfollow', unfollow_path(@service, @followers.first.id) %>
          <%end%>
  <% end %>
<% end %>
