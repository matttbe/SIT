<h3>Register new managed user for "<%=@organisation.name%>"</h3>

<%= bootstrap_form_for(@user, :url => new_managed_created_path(@user)) do |f| %>

    <%= f.text_field :name , :autofocus => true, :class=>"form-control" %>
    <%= f.text_field :firstname, :class=>"form-control" %>
    <%= f.date_select(:birthdate, :start_year => Time.now.year - 100, :end_year => Time.now.year) %>
<div style="display:none"><%= f.text_field :managed_org_id,  content:@user.managed_org_id %></div>
  <div><%= f.submit "Register" %></div>
<% end %>

